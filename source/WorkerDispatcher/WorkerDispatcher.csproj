<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Khapalov Sergei</Authors>
    <PackageProjectUrl>https://github.com/khapalov/WorkerDispatcher</PackageProjectUrl>
    <RepositoryUrl>https://github.com/khapalov/WorkerDispatcher</RepositoryUrl>
    <PackageTags>worker dispatcher async</PackageTags>
    <Version>2.1.0</Version>
    <Description>Post async work
supports several modes

Parallel -&gt; all task call as parallel
Sequenced -&gt; all task call as sequenced
ParallelLimit -&gt; all worker executed as parallel with limit count</Description>
    <RepositoryType>GitHub</RepositoryType>
    <PackageReleaseNotes>Add inline post worker t.Post(ct=&gt;Task.Delay(500, ct));</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>../../build/Debug</OutputPath>
    <DefineConstants>TRACE;DEBUG;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>../../build/Release</OutputPath>
  </PropertyGroup>

</Project>
